<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Components\Pages\HomeUsuario.razor.css" />
    <None Remove="Components\Pages\RegisterPage.razor.css" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Components\Pages\HomeUsuario.razor.css" />
    <Content Include="Components\Pages\RegisterPage.razor.css" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Blazored.Toast" Version="4.2.1" />
    <PackageReference Include="Microsoft.EntityFrameworkcore.sqlserver" Version="8.0.3" />
    <PackageReference Include="Microsoft.EntityFrameworkcore.tools" Version="8.0.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
